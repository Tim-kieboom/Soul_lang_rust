fors() {
    
    value := 0
    for i in 0..10 {
        
        if i == 5 {
            break
        }
        value += i
    }

    for 0..10 {
        Print(".")
    }

    for i in [1,2,3] {
        Print(f"{i}")
        value += i
    }

    for (key, value) in [("hello", 1), ("world", 2)] {
        Println(f"{key}: {value}")
    }
}


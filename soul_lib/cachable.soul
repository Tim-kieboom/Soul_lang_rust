union Op<T> (
	Value(T),
	Empty()
) 
{

	this::catch<T>(this@) bool {
		return match *this {
			Op<T>.Value(..) => false
			Op<T>.Empty() => true
		}
	} 
}

union Res<T, E> (
	Ok(T),
	Err(E),
) 
{

	this::catch<T, E>(this@) bool {
		return match *this {
			Res<T, E>.Ok(..) => false
			Res<T, E>.Err(..) => true
		}
	} 
}





